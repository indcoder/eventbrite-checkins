{
  "name": "eb-checkin-module",
  "version": "4.0.0",
  "description": "Module to return the check-in status of registrants for an Eventbrite event",
  "main": "dist/ebcheckins.umd.js",
  "typings": "dist/types/ebcheckins.d.ts",
  "module": "dist/ebcheckins.es5.js",
  "source": "src/ebcheckins.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:watch": "tsc --watch",
    "start": "rollup -c rollup.config.ts -w",
    "test": "snyk test && jest --coverage",
    "test:watch": "jest --coverage --watch ",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "commitmsg": "validate-commit-msg",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "checkin",
    "check-in",
    "eventbrite"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Augustine Correa <augustine.correa@hackmum.in> (http://hackmum.in)",
  "repository": {
    "type": "git",
    "url": "https://github.com/indcoder/eventbrite-checkins"
  },
  "license": "ISC",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/debug": "4.1.5",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.16",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.11.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "factory.ts": "^0.5.0",
    "faker": "^4.1.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^9.4.1",
    "lodash.camelcase": "^4.3.0",
    "microbundle": "^0.11.0",
    "nock": "^10.0.2",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.16.5",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.0",
    "semantic-release": "^15.13.18",
    "snyk": "^2.0.0",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.0.3",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "typed-rest-client": "^1.5.0"
  }
}
